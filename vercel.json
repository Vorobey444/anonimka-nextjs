{
  "buildCommand": "next build",
  "functions": {
    "src/app/api/**/*.ts": {
      "maxDuration": 30
    }
  },
  "crons": [
    {
      "path": "/api/cleanup",
      "schedule": "0 3 * * *"
    }
  ],
  "redirects": [
    {
      "source": "/:path*",
      "has": [
        {
          "type": "host",
          "value": "anonimka.online"
        }
      ],
      "destination": "https://anonimka.kz/:path*",
      "permanent": true
    },
    {
      "source": "/:path*",
      "has": [
        {
          "type": "host",
          "value": "www.anonimka.online"
        }
      ],
      "destination": "https://anonimka.kz/:path*",
      "permanent": true
    }
  ]
}